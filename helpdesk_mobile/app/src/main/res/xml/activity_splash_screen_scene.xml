<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4500"
        motion:motionInterpolator="easeInOut"
        motion:autoTransition="animateToEnd">
        <KeyFrameSet >
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="4"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="0"
                android:scaleX="0"
                android:scaleY="0" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="2"
                android:scaleX="1.1"
                android:scaleY="1.1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="4"
                android:scaleX="1"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="14"
                android:translationY="0dp" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="22"
                android:translationY="-25dp" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="100"
                android:translationY="-25dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView"
                motion:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView"
                motion:framePosition="14"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView"
                motion:framePosition="20"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView"
                motion:framePosition="100"
                android:translationY="-28dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView"
                motion:framePosition="14"
                android:translationY="-5dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView"
                motion:framePosition="24"
                android:translationY="-28dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="24"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="38"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="14"
                android:translationY="0dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="23"
                android:translationY="-32dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="100"
                android:translationY="-32dp" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/logo_app"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="161dp"
            android:layout_height="161dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.523"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="@+id/logo_app"
            android:layout_width="172dp"
            android:layout_height="35dp"
            motion:layout_constraintTop_toBottomOf="@+id/logo_app"
            motion:layout_constraintStart_toStartOf="@+id/logo_app"
            android:layout_marginTop="-43dp" />
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            android:layout_width="192dp"
            android:layout_height="18dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/logo_app"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="161dp"
            android:layout_height="161dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.523"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="@+id/logo_app"
            android:layout_width="172dp"
            android:layout_height="35dp"
            motion:layout_constraintTop_toBottomOf="@+id/logo_app"
            motion:layout_constraintStart_toStartOf="@+id/logo_app"
            android:layout_marginTop="-43dp" />
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            android:layout_width="192dp"
            android:layout_height="18dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView" />
    </ConstraintSet>
</MotionScene>